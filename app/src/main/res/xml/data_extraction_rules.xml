<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!-- 云备份规则（如 Google Drive） -->
    <cloud-backup>
        <!-- 禁止所有数据备份到云端 -->
        <exclude all="true" />
    </cloud-backup>

    <!-- 设备迁移规则（如换机时数据传输） -->
    <device-transfer>
        <!-- 禁止所有数据在设备间迁移 -->
        <exclude all="true" />
    </device-transfer>
</data-extraction-rules>